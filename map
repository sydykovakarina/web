<script>

	var mymap = L.map('mapid').setView([57.156833, 65.524854], 13);

	L.tileLayer('https://api.tiles.mapbox.com/v4/{id}/{z}/{x}/{y}.png?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6ImNpejY4NXVycTA2emYycXBndHRqcmZ3N3gifQ.rJcFIG214AriISLbB6B5aw', {
		maxZoom: 18,
		attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors, ' +
			'<a href="https://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, ' +
			'Imagery © <a href="https://www.mapbox.com/">Mapbox</a>',
		id: 'mapbox.streets'
	}).addTo(mymap);
var LeafIcon = L.Icon.extend({
		options: {
			shadowUrl: 'leaf-shadow.png',
			iconSize:     [38, 95],
			shadowSize:   [50, 64],
			iconAnchor:   [22, 94],
			shadowAnchor: [4, 62],
			popupAnchor:  [-3, -76]
		}
	});

	var greenIcon = new LeafIcon({iconUrl: 'leaf-green.png'});

	L.marker([57.166833, 65.524854], {icon: greenIcon}).addTo(mymap);

	L.circle([57.147600, 65.535800], {
		color: 'red',
		fillColor: '#f03',
		fillOpacity: 0.5,
		radius: 500
	}).addTo(mymap);

	L.polygon([
		[57.16, 65.51],
		[57.17, 65.52],
		[57.18, 65.49]
	]).addTo(mymap);

	L.marker([57.176833, 65.534854]).addTo(mymap);


	L.circle([57.157600, 65.575800], {
		color: 'green',
		fillColor: '#f03',
		fillOpacity: 0.5,
		radius: 200
	}).addTo(mymap);

	L.polygon([
		[57.14, 65.53],
		[57.17, 65.54],
		[57.17, 65.56],
		[57.18, 65.58]
	]).addTo(mymap);

	L.marker([57.175, 65.544]).addTo(mymap)
	.bindPopup("<b>Hello world!</b><br>I am a popup.").openPopup();

	L.circle([57.1612, 65.5658], {
		color: 'black',
		fillColor: '#00FFFF',
		fillOpacity: 0.5,
		radius: 350
	}).addTo(mymap)
.bindPopup("kryjochek.");

	L.polygon([
		[57.13, 65.43],
		[57.139, 65.44],
		[57.149, 65.46],
		[57.148, 65.55]
	]).addTo(mymap)
.bindPopup("good life.");

    L.control.scale().addTo(map);

    setInterval(function(){
        map.setView([0, 0]);
        setTimeout(function(){
            map.setView([60, 0]);
        }, 2000);
    }, 4000);





</script>





